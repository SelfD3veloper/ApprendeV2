<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/feed_card"
        android:layout_width="match_parent"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:layout_height="wrap_content"
        android:background="#9900aaff"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/feed_title"
            android:layout_width="fill_parent"
            android:layout_height="24dp"
            android:layout_marginLeft="22dp"
            android:layout_weight="1"
            android:gravity="center"
            android:text="Result of user"
            android:textColor="#ffffff"
            android:textSize="20sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Level"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_level"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginRight="22dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Questions"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_questions"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginRight="22dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Errors"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_error"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginRight="22dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="22dp"
                android:layout_marginLeft="24dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Points"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_points"
                android:layout_width="fill_parent"
                android:layout_height="22dp"
                android:layout_marginRight="24dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Position"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_position"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginRight="22dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Best of you"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_best"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginRight="22dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/text_feed"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginLeft="22dp"
                android:layout_weight="1"
                android:gravity="center|left"
                android:text="Bellow you"
                android:textColor="#ffffff"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/feed_bellow"
                android:layout_width="fill_parent"
                android:layout_height="24dp"
                android:layout_marginRight="22dp"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text="Hard"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </LinearLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="11dp" />
    </LinearLayout>

    <ListView
        android:id="@+id/feed_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </ListView>

</LinearLayout>